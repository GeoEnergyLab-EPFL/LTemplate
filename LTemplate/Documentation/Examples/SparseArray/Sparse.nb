Cell[TextData[{
 "A sparse array is specified as ",
 Cell[BoxData[
  RowBox[{"LType", "[", 
   RowBox[{"SparseArray", ",", "type"}], "]"}]],
  FormatType->"StandardForm"],
 " in a template, where ",
 Cell[BoxData[
  FormBox["type", TraditionalForm]]],
 " may be one of ",
 Cell[BoxData["Integer"],
  FormatType->"StandardForm"],
 ", ",
 Cell[BoxData["Real"],
  FormatType->"StandardForm"],
 ", or ",
 Cell[BoxData["Complex"],
  FormatType->"StandardForm"],
 ". A rank may optionally be specified too: ",
 Cell[BoxData[
  RowBox[{"LType", "[", 
   RowBox[{"SparseArray", ",", "type", ",", "rank"}], "]"}]],
  FormatType->"StandardForm"],
 "."
}], "Text",
