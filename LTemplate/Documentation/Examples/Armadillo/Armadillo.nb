Cell[BoxData[
 RowBox[{
  RowBox[{"template", "=", "\[IndentingNewLine]", 
   RowBox[{"LClass", "[", 
    RowBox[{"\"\<Arma\>\"", ",", "\[IndentingNewLine]", 
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"LFun", "[", 
        RowBox[{"\"\<inv\>\"", ",", 
         RowBox[{"{", 
          RowBox[{"{", 
           RowBox[{"Real", ",", "2", ",", "\"\<Constant\>\""}], "}"}], "}"}], 
         ",", 
         RowBox[{"{", 
          RowBox[{"Real", ",", "2"}], "}"}]}], "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"LFun", "[", 
        RowBox[{"\"\<eigs\>\"", ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"LType", "[", 
              RowBox[{"SparseArray", ",", "Real", ",", "2"}], "]"}], ",", 
             "\"\<Constant\>\""}], "}"}], ",", "Integer"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"Complex", ",", "1"}], "}"}]}], "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"LFun", "[", 
        RowBox[{"\"\<sprandu\>\"", ",", 
         RowBox[{"{", 
          RowBox[{"Integer", ",", "Integer", ",", "Real"}], "}"}], ",", 
         RowBox[{"LType", "[", 
          RowBox[{"SparseArray", ",", "Real"}], "]"}]}], "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"LFun", "[", 
        RowBox[{"\"\<solve\>\"", ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"Real", ",", "2", ",", "\"\<Constant\>\""}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"Real", ",", "1", ",", "\"\<Constant\>\""}], "}"}]}], 
          "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"Real", ",", "1"}], "}"}]}], "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"LFun", "[", 
        RowBox[{"\"\<print\>\"", ",", 
         RowBox[{"{", 
          RowBox[{"{", 
           RowBox[{"Real", ",", "2", ",", "\"\<Constant\>\""}], "}"}], "}"}], 
         ",", "\"\<Void\>\""}], "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{"LFun", "[", 
