   RowBox[{"\"\<CompileOptions\>\"", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{"\"\<-std=c++14\>\"", ",", "\"\<-framework Accelerate\>\""}], 
     "}"}]}]}], "\[IndentingNewLine]", "]"}]], "Input",
