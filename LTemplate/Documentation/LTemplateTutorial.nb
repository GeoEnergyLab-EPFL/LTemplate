 RowBox[{
  RowBox[{"meanVariance", "[", "arr_", "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"obj", "=", 
      RowBox[{"Make", "[", "MeanVariance", "]"}]}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"obj", "@", 
      RowBox[{"\"\<compute\>\"", "[", "arr", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"obj", "@", 
        RowBox[{"\"\<mean\>\"", "[", "]"}]}], ",", 
       RowBox[{"obj", "@", 
        RowBox[{"\"\<variance\>\"", "[", "]"}]}]}], "}"}]}]}], 
